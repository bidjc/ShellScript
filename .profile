if [ -f ~/.bashrc ]; then
. ~/.bashrc
fi
alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias ll='ls -lh --color=auto'
alias ls='ls -CF --color=auto'
alias limax='source /opt/fsl-qt/host-env'
alias lm335x='source /opt/ti-sdk-am335x-evm-07.00.00.00/env'
alias lmyset='source ~/.profile'
alias setmy='vi ~/.profile'
alias more='more'
PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:./

